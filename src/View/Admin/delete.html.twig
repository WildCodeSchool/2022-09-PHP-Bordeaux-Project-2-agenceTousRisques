{% block stylesheet %}
    <link rel="stylesheet" href="/assets/css/delete.css">
    <link rel="stylesheet" href="/assets/css/style.css">
{% endblock %}
{% block content %}
    <div class="container">
        <div class="form">
            <h2 class="title-deleteUSer">Supprimer un utilisateur</h2>
            <form action='delete' method='post' class="deleteUser">
                <div class="deleteformerror">
                    {% for messages in message %}
                        <li>{{ messages }}</li>
                    {% endfor %}
                </div>
                <label for="email"> Email: </label>
                <input type='email' id='email' name='email'>
                <button type='submit'> Supprimer</button>
            </form>
        </div>
    </div>
{% endblock %}











